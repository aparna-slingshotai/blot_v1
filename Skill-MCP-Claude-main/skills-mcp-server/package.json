{
  "name": "skills-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for skill discovery and retrieval with Express REST API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "skills-mcp-server": "dist/index.js",
    "skills-api": "dist/api.js",
    "skills-mcp-sse": "dist/sse.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:api": "node dist/api.js",
    "start:sse": "node dist/sse.js",
    "dev": "tsx watch src/index.ts",
    "dev:api": "tsx watch src/api.ts",
    "dev:sse": "tsx watch src/sse.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "archiver": "^7.0.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "multer": "^1.4.5-lts.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "mcp",
    "skills",
    "claude",
    "anthropic"
  ],
  "author": "",
  "license": "MIT"
}
