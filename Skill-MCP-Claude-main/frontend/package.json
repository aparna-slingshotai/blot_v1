  "devDependencies": {
    "vitest": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "jsdom": "^25.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "claude",
    "mcp",
    "skills",
    "ai"
